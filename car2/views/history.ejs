    <script>
        function formatDate(dateStr) {
            const date = new Date(dateStr);
            return date.toLocaleDateString("th-TH", {
                day: 'numeric', month: 'numeric', year: 'numeric'
            });
        }

        function toThaiDateTime(dateStr) {
            const d = new Date(dateStr);
            return d.toLocaleDateString("th-TH", {
                day: 'numeric', month: 'long', year: 'numeric'
            }) + ' ‡πÄ‡∏ß‡∏•‡∏≤ ' + d.toLocaleTimeString("th-TH").replace(/:\d+$/, "");
        }

        async function initializeLiff() {
            try {
                await liff.init({ liffId: "2007063611-2LjAaylQ" });
                if (!liff.isLoggedIn()) {
                    liff.login();
                } else {
                    const profile = await liff.getProfile();
                    const userId = profile.userId;
                    console.log("üì¢ userId:", userId);

                    $("#plateSelector").on("change", () => {
                        const selected = $("#plateSelector").val();
                        if (selected) loadHistory(userId, selected);
                    });
                }
            } catch (error) {
                console.error("üî• LIFF Error:", error);
            }
        }

        function loadHistory(userId, selectedPlate) {
            $.get(`/checklist-history/${userId}`, function (entries) {
                const filtered = entries.filter(e => e.plate_number === selectedPlate);
                if (!filtered.length) {
                    $("#historyTableContainer").html('<div class="alert alert-warning">‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•</div>');
                    $("#latestInfo").html("");
                    return;
                }

                const latest = filtered[0];
                const submittedAt = toThaiDateTime(latest.submitted_at);
                const latestHtml = `
                    <p>üìã ‡∏ï‡∏£‡∏ß‡∏à‡∏™‡∏≠‡∏ö‡πÇ‡∏î‡∏¢: ${latest.inspector}</p>
                    <p>üìÖ ‡∏ß‡∏±‡∏ô‡∏ó‡∏µ‡πà: ${submittedAt}</p>
                    <p>üöó ‡∏õ‡πâ‡∏≤‡∏¢‡∏ó‡∏∞‡πÄ‡∏ö‡∏µ‡∏¢‡∏ô: ${latest.plate_number}</p>
                `;
                $("#latestInfo").html(latestHtml);

                const checklist = checklists[latest.plate_number] || [];
                const dates = [...new Set(filtered.map(e => formatDate(e.submitted_at)))];

                // ‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÇ‡∏Ñ‡∏£‡∏á‡∏™‡∏£‡πâ‡∏≤‡∏á‡πÄ‡∏Å‡πá‡∏ö‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡πÅ‡∏¢‡∏Å‡∏´‡∏°‡∏ß‡∏î
                const categoryMap = {};
                checklist.forEach(cat => {
                    categoryMap[cat.category] = cat.details.map(d => d.name);
                });

                const statusMap = {};
                Object.keys(categoryMap).forEach(cat => {
                    statusMap[cat] = {};
                    categoryMap[cat].forEach(name => {
                        statusMap[cat][name] = {};
                    });
                });

                filtered.forEach(entry => {
                    const date = formatDate(entry.submitted_at);
                    const equipment = typeof entry.equipment === "string" ? JSON.parse(entry.equipment) : entry.equipment;
                    const itemMap = {};
                    equipment.forEach(item => itemMap[item.name] = item);

                    checklist.forEach(cat => {
                        cat.details.forEach(d => {
                            const item = itemMap[d.id];
                            const qty = item?.quantity || 0;
                            const expected = d.expected || 0;
                            let status = qty > 0 ? `‡∏°‡∏µ ${qty}` : "‡πÑ‡∏°‡πà‡∏°‡∏µ";
                            if (expected > 0) {
                                if (qty === expected) status += " ‡∏Ñ‡∏£‡∏ö (check)";
                                else if (qty < expected) status += ` ‡∏Ç‡∏≤‡∏î ${expected - qty} (-)`;
                            }
                            if (!item) status = `‡πÑ‡∏°‡πà‡∏°‡∏µ ‡∏Ç‡∏≤‡∏î ${expected || 1} (-)`;
                            statusMap[cat.category][d.name][date] = status;
                        });
                    });
                });

                let html = '<table class="table table-bordered"><thead><tr><th>‡∏´‡∏°‡∏ß‡∏î / ‡∏≠‡∏∏‡∏õ‡∏Å‡∏£‡∏ì‡πå</th>';
                dates.forEach(d => html += `<th>${d}</th>`);
                html += '</tr></thead><tbody>';

                Object.entries(statusMap).forEach(([cat, items]) => {
                    html += `<tr><td colspan="${dates.length + 1}"><strong>${cat}</strong></td></tr>`;
                    Object.entries(items).forEach(([name, statusByDate]) => {
                        html += `<tr><td>${name}</td>`;
                        dates.forEach(d => html += `<td>${statusByDate[d] || '-'}</td>`);
                        html += '</tr>';
                    });
                });

                html += '</tbody></table>';
                $("#historyTableContainer").html(html);
            });
        }

        initializeLiff();
